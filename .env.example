# ============================================
# GEMINI API CONFIGURATION
# ============================================

# Primary Gemini API key (required)
AI_INTEGRATIONS_GEMINI_API_KEY=your_gemini_api_key_here

# Additional Gemini API keys for rate limiting (optional, comma-separated)
GEMINI_API_KEYS=

# Gemini base URL (optional)
AI_INTEGRATIONS_GEMINI_BASE_URL=

# ============================================
# PINECONE CONFIGURATION
# ============================================

# Pinecone API key (get from: https://app.pinecone.io/)
PINECONE_API_KEY=your_pinecone_api_key_here

# Pinecone index name (default: reversr-innovations)
PINECONE_INDEX_NAME=reversr-innovations

# ============================================
# SERVER CONFIGURATION
# ============================================

# API server port
API_PORT=5000
